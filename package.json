{
  "name": "wonderful-log",
  "version": "1.0.7",
  "description": "An improved logging system for Node.js",
  "main": "./dist/index.js",
  "scripts": {
    "test": "npm run prepare && ./node_modules/mocha/bin/mocha test/ --recursive --timeout=10000",
    "prepare": "node -e \"require('prepare-package')()\"",
    "prepare:watch": "nodemon -w ./src -e '*' --exec 'npm run prepare'"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/itw-creative-works/wonderful-log"
  },
  "keywords": [
    "rest api"
  ],
  "author": "ITW Creative Works",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/itw-creative-works/wonderful-log/issues"
  },
  "homepage": "https://itwcreativeworks.com",
  "runkitExampleFilename": "examples/process.js",
  "preparePackage": {
    "input": "./src",
    "output": "./dist",
    "replace": {}
  },
  "dependencies": {
    "itwcw-package-analytics": "^1.0.4"
  },
  "devDependencies": {
    "mocha": "^8.4.0",
    "node-powertools": "^1.4.4",
    "prepare-package": "^1.1.12"
  }
}